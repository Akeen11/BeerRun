@{
    ViewData["Title"] = "Home Page";
}

<style>
    /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
    #map {
        height: 500px;
        width: 500px;
    }
</style>

<div class="row">
    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: { lat: 38.140320, lng: -98.912742},
          zoom: 4
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBHIpXOrVIHPE5noiVFfqifzI5r0MeCvQM&callback=initMap"
            async defer></script>
</div>
